{
  "editor.tabSize": 2,
  "editor.fontSize": 16,
  "terminal.integrated.defaultProfile.osx": "fish",
  "git.blame.editorDecoration.enabled": true,
  "editor.fontLigatures": "'calt', 'liga'",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "elixirLS.suggestSpecs": false,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "git.autofetch": true,
  "git.openRepositoryInParentFolders": "always",
  "workbench.iconTheme": "catppuccin-mocha",
  "git.confirmSync": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "explorer.confirmDelete": false,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDragAndDrop": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "cursor.composer.collapsePaneInputBoxPills": true,
  "workbench.colorCustomizations": {
    "editorStickyScroll.shadow": "#181e28",
    "scrollbar.shadow": "#181e28"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.renderWhitespace": "none",
  "editor.semanticHighlighting.enabled": true,
  "terminal.integrated.fontSize": 16,
  "cursor.cpp.disabledLanguages": ["markdown"],
  "cursor.composer.shouldChimeAfterChatFinishes": true,
  "workbench.layoutControl.enabled": false,
  "githubPullRequests.pullBranch": "never",
  "npm.keybindingsChangedWarningShown": true,
  "window.customTitleBarVisibility": "never",
  "cursor.enable_agent_window_setting": true,
  "diffEditor.renderSideBySide": false,
  "cursor.composer.usageSummaryDisplay": "never",
  "cursor.composer.textSizeScale": 1.15,
  "editor.fontFamily": "Maple Mono NF",
  "update.releaseTrack": "dev",
  "update.mode": "silentlyApplyOnQuit",
  "cursor.enable_git_worktrees_setting": true,
  "cursor.agent_layout_browser_beta_setting": true
}
