{
  "editor.fontFamily": "MonaspiceNe Nerd Font",
  "editor.tabSize": 2,
  "editor.fontSize": 16,
  "update.releaseTrack": "dev",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "git.blame.editorDecoration.enabled": true,
  "editor.fontLigatures": "'calt', 'liga'",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "elixirLS.suggestSpecs": false,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "git.autofetch": true,
  "git.openRepositoryInParentFolders": "always",
  "workbench.iconTheme": "catppuccin-latte",
  "git.confirmSync": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "explorer.confirmDelete": false,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDragAndDrop": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "cursor.composer.collapsePaneInputBoxPills": true,
  "workbench.colorCustomizations": {
    "editorStickyScroll.shadow": "#181e28",
    "scrollbar.shadow": "#181e28"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.renderWhitespace": "none",
  "editor.semanticHighlighting.enabled": true,
  "cursor.composer.usageSummaryDisplay": "never",
  "workbench.editor.showTabs": "single",
  "workbench.activityBar.orientation": "vertical",
  "terminal.integrated.fontSize": 16,
  "cursor.cpp.disabledLanguages": ["markdown"],
  "cursor.composer.shouldChimeAfterChatFinishes": true,
  "workbench.layoutControl.enabled": false,
  "workbench.sideBar.location": "right"
}
