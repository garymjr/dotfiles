{
  "assistant": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-5-sonnet-20240620"
    },
    "version": "2"
  },
  "buffer_font_family": "Maple Mono NF",
  "buffer_font_size": 17.5,
  "current_line_highlight": "none",
  "cursor_blink": false,
  "experimental.theme_overrides": {
    "editor.document_highlight.read_background": "#2b2f39",
    "editor.document_highlight.bracket_background": "#393f4d",
    "editor.foreground": "#a5accf",
    "editor.highlighted_line.background": "#000000",
    "players": [
      {
        "cursor": "#74ade8ff",
        "background": "#74ade8ff",
        "selection": "#74ade83d"
      },
      {
        "cursor": "#be5046ff",
        "background": "#be5046ff",
        "selection": "#be50463d"
      },
      {
        "cursor": "#bf956aff",
        "background": "#bf956aff",
        "selection": "#bf956a3d"
      },
      {
        "cursor": "#b477cfff",
        "background": "#b477cfff",
        "selection": "#b477cf3d"
      },
      {
        "cursor": "#6eb4bfff",
        "background": "#6eb4bfff",
        "selection": "#6eb4bf3d"
      },
      {
        "cursor": "#d07277ff",
        "background": "#d07277ff",
        "selection": "#d072773d"
      },
      {
        "cursor": "#dec184ff",
        "background": "#dec184ff",
        "selection": "#dec1843d"
      },
      {
        "cursor": "#a1c181ff",
        "background": "#a1c181ff",
        "selection": "#a1c1813d"
      }
    ],
    "syntax": {
      "boolean": {
        "color": "#d0679d"
      },
      "comment": {
        "color": "#868cad",
        "font_style": "italic"
      },
      // "constant": {
      //   "color": "#d0679d"
      // },
      "function": {
        "color": "#add7ff"
      },
      "keyword": {
        "color": "#5de4c7"
      },
      "operator": {
        "color": "#add7ff"
      },
      "string.special.symbol": {
        "color": "#e4f0fb"
      },
      "variable": {
        "color": "#000000"
      },
      "type": {
        "color": "#e4f0fb"
      }
    },
    "predictive": "#6a7199"
  },
  "features": {
    "inline_completion_provider": "supermaven"
  },
  "file_types": {
    "Dockerfile": ["Dockerfile", "dockerfile.*"],
    "JSON": ["json", "jsonc", "*.code-snippets"]
  },
  "format_on_save": "off",
  "language_servers": ["!elixir-ls", "!next-ls", "lexical", "!eslint", "..."],
  "multi_cursor_modifier": "cmd_or_ctrl",
  "outline_panel": {
    "button": false
  },
  "pane_split_direction_horizontal": "down",
  "pane_split_direction_vertical": "right",
  "project_panel": {
    "button": false,
    "indent_size": 10
  },
  "relative_line_numbers": true,
  "restore_on_startup": "none",
  "scrollbar": {
    "show": "never"
  },
  "tab_bar": {
    "show_nav_history_buttons": false,
    "show": true
  },
  "tabs": { "file_icons": true },
  "toolbar": {
    "quick_actions": false
  },
  "terminal": {
    "button": false,
    "font_family": "Maple Mono NF",
    "env": {
      "EDITOR": "zed",
      "GIT_EDITOR": "zed --wait"
    }
  },
  "theme": {
    "mode": "dark",
    "light": "Nebula Glow",
    "dark": "poimandres storm"
  },
  "ui_font_family": "Zed Plex Mono",
  "ui_font_size": 18,
  "use_on_type_format": false,
  "use_smartcase_search": true,
  "use_system_path_prompts": false,
  "vim_mode": true,
  "when_closing_with_no_tabs": "close_window",
  "start_in_fullscreen": true
}
