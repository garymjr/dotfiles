[
  {
    "context": "Pane > Editor || Terminal",
    "bindings": {
      "cmd-shift-i": "assistant::ToggleFocus"
    }
  },
  {
    "context": "AssistantPanel > ContextEditor",
    "bindings": {
      "cmd-shift-i": "workspace::ToggleRightDock"
    }
  },
  {
    "bindings": {
      "cmd-,": "zed::OpenSettings"
    }
  },
  {
    "context": "Editor && vim_mode == normal && !menu",
    "bindings": {
      "g d": "editor::GoToDefinition",
      "g h": "editor::MoveToBeginningOfLine",
      "g l": "editor::MoveToEndOfLine",
      "g r i": "editor::GoToImplementation",
      "g r r": "editor::FindAllReferences",
      "g r n": "editor::Rename"
    }
  },
  {
    "context": "Editor && vim_mode == visual && !menu",
    "bindings": {
      "<": "editor::Outdent",
      ">": "editor::Indent",
      "g n": "vim::SelectNext",
      "shift-j": "editor::MoveLineDown",
      "shift-k": "editor::MoveLineUp"
    }
  },
  {
    "context": "ProjectSearchBar",
    "bindings": {
      "escape": "pane::CloseActiveItem"
    }
  }
]
